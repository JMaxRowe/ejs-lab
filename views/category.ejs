<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title><%= category %> Menu</title>
</head>
<body>
  <%- include('./partials/nav') %>

  <h1><%= category %> Menu</h1>
  <ul>
    <% menuItems.forEach(item => { %>
      <li>
        <p><%= item.name %> - <%= item.price%></p> 
      </li>
    <% }) %>
  </ul>
</body>
</html>

